// xbl_uefi_mem address get from boot_images Core.dsc.inc file: PcdEmbeddedFdBaseAddress + SERIAL_BUFFER_OFFSET,0x5FC00000+0x3F7000
// kernel boot log: 0x5FC00000~ 0x5FFF7000
&reserved_memory {
kboot_uboot_logmem:
	kboot_uboot_logmem@0x5FC00000 {
		reg = <0x0 0x5FC00000 0x0 0x3FF000>;
		compatible = "oppo,xbl_uefi_kbootlog";
		xbluefi - offset = <0x3F7000>;
		no - map;
	};

ramoops:
	ramoops@0x0x66800000 {
		compatible = "ramoops";
		reg = <0 0x66800000 0 0x00400000>;
		record - size =  <0x40000>; //256x1024
		console - size = <0x40000>;
		ftrace - size =  <0x40000>;
		pmsg - size =         <0x200000>;
		devinfo - size =  <0x01000>;
		ecc - size =          <0x0>;
	};
};
//bengal-qrd.dtsi
&soc {
	gpio_keys {
		vol_up {
			/ delete - property / label;
			label = "volume_down";
			/ delete - property / linux, code;
			linux, code = <KEY_VOLUMEDOWN>;
		};
	};

	/* Ping.Liu@Bsp.Fingerprint.Secure 2019-10-16 add for secure proc */
	oplus_secure_common {
		compatible = "oplus,secure_common";
		oplus, sec_reg_num = <0x1B405D8>;
		oplus, sec_en_anti_reg = <0x1B401CC>;
		oplus, sec_override1_reg = <0x7860C0>;
		oplus, override1_en_value = <0x1>;
	};

	devinfo {
		status = "disabled";
	};
	devinfo_panther {
		compatible = "oplus-devinfo";

		pinctrl - names = "default", "sleep", "active", "idle";
		pinctrl - 0 = <&hw_pins_default>;
		pinctrl - 1 = <&aboard_pin_gpio0_sleep>;
		pinctrl - 2 = <&aboard_pin_gpio0_active>;
		pinctrl - 3 = <&aboard_pin_gpio0_idle>;
		main - sub {
			use_tristate_gpio;
			compatible = "hw-match, main-sub";
			aboard - gpio0 = <&tlmm 99 0>;
			aboard - patterns = <1 0 0>;  /*0[high-resistance] 1[pull-down] 2[pull-up]*/
			/*main board*/
			match - projects = <0x2172F 21730 21731>;
		};
	};

	/* Fuchun.Liao@BSP.CHG.Basic 2018/11/26 modify for oppo rf cable monitor */
	oplus_rf_cable {
		compatible = "oplus,rf_cable";
		interrupt - parent = <&tlmm>;
		rf_cable_support_num = <1>;
		rf, cable0 - gpio = <&tlmm 106 0>;
		rf, cable - support - timer = <0>;
		pinctrl - names = "rf_cable_active";
		pinctrl - 0 = <&rf_cable0_active>;
	};

	shell_front {
		compatible = "oplus,shell-temp";
	};

	shell_frame {
		compatible = "oplus,shell-temp";
	};

	shell_back {
		compatible = "oplus,shell-temp";
	};

	oplus_sim_detect {
		compatible = "oplus, sim_detect";
		Hw, sim_det = <&tlmm 79 0>;
	};
};

&pm6125_gpios {
	disp_pins {
	sde_vddr_en:
		sde_vddr_en {
			pins = "gpio2";
			function = "normal";
			output - enable;
			output - high;
			bias - pull - up;
			qcom, drive - strength = <2>;
			//power-source = <0>;
		};
	};
};


&dsi_s6e3fc3_samsung_amoled_cmd {
	qcom, panel - supply - entries = <&dsi_panel_pwr_supply_no_labibb>;
	qcom, mdss - dsi - bl - pmic - control - type = "bl_ctrl_dcs";
	qcom, mdss - dsi - bl - min - level = <1>;
	qcom, mdss - dsi - bl - max - level = <4095>;
	qcom, mdss - brightness - max - level = <4095>;
	qcom, mdss - dsi - bl - normal - max - level = <2047>;
	qcom, mdss - brightness - normal - max - level = <2047>;
	qcom, mdss - dsi - bl - hbm - min - level = <3727>;

qcom, mdss - brightness - default-level = <400>;
	qcom, platform - te - gpio = <&tlmm 81 0>;
	qcom, platform - reset - gpio = <&tlmm 82 0>;
	qcom, platform - panel - vddi - gpio = <&tlmm 103 0>;
	qcom, platform - panel - vci - gpio = <&tlmm 71 0>;
	qcom, platform - panel - vddr - gpio = <&pm6125_gpios 2 0>;
	qcom, dsi - select - clocks = "mux_byte_clk0", "mux_pixel_clk0";
};

&spmi_bus {
	qcom, pm6125@0 {
		qcom, power - on@800 {
			qcom, pmicwd;
			qcom, pon_2 {
				qcom, pon - type = <1>;
				qcom, pull - up = <1>;
				linux, code = <KEY_VOLUMEUP>;
			};
		};
	};
};
&tlmm {
	aboard_pin {
		hw_pins_default:
		hw_pins_default {
		};

	aboard_pin_gpio0_sleep:
		aboard_pin_gpio0_sleep {
			mux {
				pins = "gpio99";
				function = "gpio";
			};
			config {
				pins = "gpio99";
				bias - pull - down;
				input - enable;
			};
		};
	aboard_pin_gpio0_active:
		aboard_pin_gpio0_active {
			mux {
				pins = "gpio99";
				function = "gpio";
			};
			config {
				pins = "gpio99";
				bias - pull - up;
				input - enable;
			};
		};

	aboard_pin_gpio0_idle:
		aboard_pin_gpio0_idle {
			mux {
				pins = "gpio99";
				function = "gpio";
			};
			config {
				pins = "gpio99";
				input - enable;
				bias - disable;
			};
		};
	};
};

